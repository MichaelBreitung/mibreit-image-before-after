{
  "name": "mibreit-image-before-after",
  "version": "0.1.0",
  "description": "Image comparison in before and after fashion",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:dev": "webpack --config webpack.config.dev.js",
    "build:npm": "rimraf lib/ && webpack --config webpack.config.npm.js",
    "build:npm-es6": "rimraf lib/ && tsc && copyfiles -u 1 src/**/*.svg src/**/*.css lib/"
  },
  "keywords": [
    "Typescript"
  ],
  "author": "Michael Breitung",
  "license": "BSD-2-Clause",
  "dependencies": {
    "mibreit-dom-tools": "github:MichaelBreitung/mibreit-dom-tools"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.2",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.1.3",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.17.0",
    "webpack-cli": "^5.0.0"
  }
}
